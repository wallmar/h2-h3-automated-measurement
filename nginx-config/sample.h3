server {
		server_name sample-00;
		listen 443 http3;
		listen 443 ssl;
		ssl_certificate     /etc/ssl/self-signed/h2-h3.crt;
		ssl_certificate_key /etc/ssl/self-signed/h2-h3.key;
		ssl_protocols TLSv1.3;
		ssl_early_data on;
		http3_max_field_size 80000;
        http3_max_table_capacity 50;
        http3_max_blocked_streams 30;

		location / {
				root samples/sample-00;
				add_header Alt-Svc '$http3=":443"; ma=86400';
				add_header Last-Modified $date_gmt;
				add_header Cache-Control 'private no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0';
				if_modified_since off;
				expires off;
				etag off;
		}
}
